<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BMSelfView">
        <attr name="hint" format="string|reference" />
        <attr name="highlightColor" format="color|reference" />
        <attr name="maxLength" format="integer" />
        <attr name="isLimited" format="boolean" />
        <attr name="singleLine" format="boolean" />
        <attr name="textSize" format="dimension|reference" />
        <attr name="type">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>
        <attr name="choice" format="string|reference" />
        <attr name="defaultChoice" format="integer" />
        <attr name="moreinfo" format="string|reference" />
        <attr name="android:inputType" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
    
    <declare-styleable name="swipelistviewstyle">
        <attr name="right_width" format="dimension"></attr>
    </declare-styleable>

</resources>